[user]
	name = Tsungyu Yu
	email = ytyubox@gmail.com
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[commit]
	template = /Users/youzongyu/.git-commit-template.txt
[credential]
    helper = osxkeychain
[alias]
	cv = commit -v
	cho = checkout
	br = branch
[core]
	excludesfile = /Users/youzongyu/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	trustExitCode = true
[pull]
	rebase = true
[push]
	default = current
